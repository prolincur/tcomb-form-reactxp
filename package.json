{
  "name": "tcomb-form-reactxp",
  "version": "0.16.13",
  "description": "ReactXP Extension that provides UI library for developing forms writing less code",
  "author": "Sourabh Soni <Sourabh.Soni@prolincur.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/prolincur/tcomb-form-reactxp.git"
  },
  "bugs": {
    "url": "https://github.com/prolincur/tcomb-form-reactxp/issues"
  },
  "homepage": "https://github.com/prolincur/tcomb-form-reactxp",
  "main": "index.js",
  "scripts": {
    "copymd": "cp -f src/native-common/*.md dist/native-common/ && cp -f src/web/*.md dist/web/",
    "copylic": "cp -f src/native-common/LICENSE dist/native-common/ && cp -f src/web/LICENSE dist/web/",
    "copy": "npm run copymd && npm run copylic",
    "build": "rm -rf dist && babel src --out-dir dist --source-maps && npm run copy"
  },
  "files": [
    "LICENSE.md",
    "README.md",
    "dist/",
    "./index.js",
    "index.ios.js",
    "index.android.js",
    "index.web.js"
  ],
  "dependencies": {
    "tcomb-form": "http://localhost:5000/tcomb-form-0.9.22.tgz",
    "tcomb-form-native": "http://localhost:5000/tcomb-form-native-0.6.20.tgz"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.34",
    "@babel/preset-env": "^7.0.0-beta.34",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^8.0.0-beta.0",
    "babel-preset-react-native": "^4.0.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "eslint": "^4.13.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "reactxp": "2.0.0",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-native": "0.59.10"
  },
  "tags": [
    "tcomb",
    "form",
    "forms",
    "reactxp"
  ],
  "keywords": [
    "tcomb",
    "form",
    "forms",
    "reactxp"
  ]
}
